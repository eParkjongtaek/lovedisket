package dec07.add;

class OperatorEx19 
{ 
      public static void main(String[] args) 
      { 
            int share = 10 / 8; //1
            int remain = 10 % 8; //2
            System.out.println("10을 8로 나누면, "); 
            System.out.println("몫은 "+share+"이고, 나머지는 "+remain+"입니다."); 
      } 
} 