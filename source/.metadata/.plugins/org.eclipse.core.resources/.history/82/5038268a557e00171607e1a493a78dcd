package aug11;

public class rotto {

	
	
	public static void main(String[] args) {
		
		int cool[] = new int[6];
		int real_cool[] = new int[6];
		int not_real_cool[] = new int[6];
		boolean chk = true;	
		
	    for (int i = 0; i < cool.length; i++) {
	    	cool[i] = (int)(Math.random() * 46) + 1;

	    	System.out.println(cool[i]);
	    	
	    	for (int j = 0; j < real_cool.length; j++) {
	    		
	    		while(true)
	    		{
	    			real_cool[i] = cool[i];
	    			if(real_cool[i] == cool[i])
	    			break;
	    		}
		    		
	    	}		    	
	    	
	    }		
		
	    for (int z = 0; z < real_cool.length; z++) 
	    {
	    	
	    	System.out.print(real_cool[z] + "\t");
	    	
	    }
	    
		
	}

}
